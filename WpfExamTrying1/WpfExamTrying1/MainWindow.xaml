                <MenuItem Height="20" Header="Find" Margin="0,0,-14,0" Click="MenuItem_Click5"/>
            </MenuItem>
        </Menu>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="10,288,0,0" VerticalAlignment="Top" Width="119" Height="42" Click="button_Click"/>
        <RichTextBox x:Name="richTextBox" HorizontalAlignment="Left" Height="241" Margin="10,42,0,0" VerticalAlignment="Top" Width="386">
